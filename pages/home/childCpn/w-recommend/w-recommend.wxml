<!--pages/home/childCpn/w-recommend/w-recommend.wxml-->
<view class="recommends">
  <block wx:for="{{recommends}}" wx:key="{{index}}">
    <view class="recommends-item">
      <image src="{{item.image}}" mode="heightFix" bindload="handleImageLoad"/>
      <text>{{item.title}}</text>
    </view>
  </block>
</view>
